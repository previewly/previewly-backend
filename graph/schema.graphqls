# GraphQL schema example
#
# https://gqlgen.com/getting-started/

interface MinimalPreviewData {
  id: Int!
  url: String!
  status: Status!
}

type PreviewData implements MinimalPreviewData {
  id: Int!
  url: String!
  status: Status!
  image: String!
}

type Query {
  getPreviewData(token: String!, url: String!): PreviewData
}

type Mutation {
  createToken: String!
  addUrl(token: String!, url: String!): MinimalPreviewData
}

enum Status {
  success
  error
  pending
}
